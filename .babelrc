{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["ie >= 11"]
        },
        "exclude": ["transform-async-to-generator", "transform-regenerator"],
        "modules": false,
        "loose": true
      }
    ]
  ],
  "plugins": ["transform-decorators-legacy", "transform-object-rest-spread"],
  "env": {
    "test": {
      "plugins": ["istanbul"]
    },
    "commonjs": {
      "presets": [
        [
          "env",
          {
            "loose": true
          }
        ]
      ]
    }
  }
}
